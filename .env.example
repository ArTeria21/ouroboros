# Required local LLM config (LM Studio OpenAI-compatible endpoint)
OUROBOROS_LLM_BASE_URL=http://host.docker.internal:1234/v1
OUROBOROS_LLM_API_KEY=lm-studio
OUROBOROS_MODEL=qwen/qwen3-30b-a3b-2507

# Required secrets
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TOTAL_BUDGET=50
GITHUB_TOKEN=your_github_token_with_repo_scope

# Required repository coordinates (your fork)
GITHUB_USER=your_github_username
GITHUB_REPO=ouroboros

# Optional web search model (only if OPENAI_API_KEY is set)
OUROBOROS_WEBSEARCH_MODEL=gpt-5
OPENAI_API_KEY=

# Optional runtime tuning
OUROBOROS_MAX_WORKERS=5
OUROBOROS_MAX_ROUNDS=200
OUROBOROS_BG_BUDGET_PCT=10
OUROBOROS_SOFT_TIMEOUT_SEC=600
OUROBOROS_HARD_TIMEOUT_SEC=1800
OUROBOROS_DIAG_HEARTBEAT_SEC=30
OUROBOROS_DIAG_SLOW_CYCLE_SEC=20
OUROBOROS_BOOT_BRANCH=ouroboros

# Optional explicit paths inside container
DRIVE_ROOT=/data/Ouroboros
REPO_DIR=/data/ouroboros_repo

